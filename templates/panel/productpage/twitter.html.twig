{% extends 'panel/panelbase.html.twig' %}



{% block stylesheets %}
<link href="{{ asset('css/productpage.css') }}" rel="stylesheet">{% endblock %}
{% block body %}

{% for page in twitter_pages %}

<figure class="snip1418"><img width="100%" height="100%" src="{{ page.imagelink }}" alt="sample85"/>
  <div class="add-to-cart"> <i class="ion-android-add"></i><span>Ýncele</span></div>
  <figcaption>
    <h3>{{ page.pageName}}</h3>
    <p>{{ page.description }}</p>
    <div class="price">
    {{ page.price }} TL
    </div>
  </figcaption><a href="{{path('panel_productpage_twitter_detail',{'id': page.id})}}"></a>
</figure>
{% endfor %}

<script src="{{ asset('js/productpage.min.js') }}"></script>
	
{% endblock %}